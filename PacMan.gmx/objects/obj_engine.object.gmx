            <string>///Place Dots
with(obj_path){
    var rc = scr_row_col(x,y);
    var row = ds_map_find_value(rc,"row");
    var col = ds_map_find_value(rc,"col");
    var place_dot = true;
    var place_powerup = false;
    //Middle Box
    if((row&gt;=9)&amp;&amp;(col&gt;=9)&amp;&amp;(row&lt;=19)&amp;&amp;(col&lt;=22)){
        place_dot = false;
    }
    //Exit Row
    else if((row=14)&amp;&amp;(col!=8)&amp;&amp;(col!=23)){
        place_dot = false;
    }
    //Pacman Spawn
    else if((row = 23) &amp;&amp; (col==15||col==16)){
        place_dot = false;
    }
    //Powerup
    else if((row == 3||row == 23) &amp;&amp; (col == 3||col == 28)){
        place_dot = false;
        place_powerup = true;
    }
    if(place_dot){
        instance_create(x,y,obj_dot);
    }
    if(place_powerup){
        instance_create(x,y,obj_powerup);
    }
}

